# 네이버 API 설정 (필수)
NAVER_CLIENT_ID=여기에_네이버_API_클라이언트_ID_입력
NAVER_CLIENT_SECRET=여기에_네이버_API_클라이언트_SECRET_입력


# 프로젝트 환경 설정
PROJECT_NAME=네이버 뉴스 검색 API
PROJECT_VERSION=1.0.0
PROJECT_DESCRIPTION=네이버 뉴스 API를 사용한 뉴스 검색 및 분석 서비스

# CORS 설정 (개발 환경)
CORS_ORIGINS=*

# 기본 뉴스 검색 설정
DEFAULT_NEWS_COUNT=100
DEFAULT_NEWS_SORT=date
DEFAULT_NEWS_DAYS=30

# 자동 다운로드 폴더 복사 설정
AUTO_COPY_TO_DOWNLOADS=true

# 서버 설정
HOST=0.0.0.0
PORT=8000
DEBUG=true

# 로깅 레벨
LOG_LEVEL=INFO
